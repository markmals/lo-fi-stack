{
    "private": true,
    "sideEffects": false,
    "scripts": {
        "dev": "run-p dev:*",
        "storybook": "npm run storybook:dev",
        "build": "run-s build:unocss build:remix",
        "start": "remix-serve build",
        "dev:remix": "remix dev",
        "dev:unocss": "npm run build:unocss -- --watch",
        "storybook:dev": "start-storybook -p 6006",
        "build:remix": "remix build",
        "build:unocss": "unocss 'app/**/*.tsx' 'app/components/**/*.css' --out-file app/styles/uno.css",
        "storybook:build": "build-storybook"
    },
    "resolutions": {
        "react": "npm:@preact/compat",
        "react-dom": "npm:@preact/compat"
    },
    "dependencies": {
        "@remix-run/node": "^1.7.2",
        "@remix-run/react": "^1.7.2",
        "@remix-run/serve": "^1.7.2",
        "preact": "^10.11.0",
        "preact-render-to-string": "^5.2.4",
        "react": "npm:@preact/compat",
        "react-dom": "npm:@preact/compat"
    },
    "devDependencies": {
        "@preact/preset-vite": "^2.4.0",
        "@remix-run/dev": "^1.7.2",
        "@storybook/addon-essentials": "^6.5.13",
        "@storybook/addon-interactions": "^6.5.13",
        "@storybook/addon-links": "^6.5.13",
        "@storybook/builder-vite": "^0.2.5",
        "@storybook/core-server": "^6.5.13",
        "@storybook/preact": "^6.5.13",
        "@storybook/testing-library": "^0.0.13",
        "@testing-library/preact": "^3.2.2",
        "eslint": "^8.23.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.0",
        "prettier-plugin-organize-imports": "^3.2.1",
        "remix-flat-routes": "^0.4.8",
        "typescript": "^4.7.4",
        "unocss": "^0.46.5",
        "vite-tsconfig-paths": "^3.6.0",
        "vitest": "^0.25.3"
    }
}
