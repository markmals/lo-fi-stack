{
    "private": true,
    "sideEffects": false,
    "type": "module",
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "scripts": {
        "dev": "npm-run-all build --parallel \"dev:*\"",
        "dev:node": "cross-env NODE_ENV=development node --require dotenv/config --loader tsx --watch ./dev-server.ts",
        "dev:remix": "remix dev",
        "hmr": "remix dev --no-restart -c 'tsx dev-server.ts'",
        "start": "tsx dev-server.ts",
        "storybook": "npm run storybook:dev",
        "format": "prettier --write .",
        "generate": "prisma generate --data-proxy",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
        "test": "vitest",
        "typecheck": "tsc",
        "build": "remix build",
        "clean": "git clean -fdX .",
        "deploy": "",
        "storybook:dev": "storybook dev -p 6006",
        "storybook:build": "storybook build",
        "prepare": "husky install",
        "postinstall": "npm run generate"
    },
    "dependencies": {
        "@fastify/send": "^2.1.0",
        "@heroicons/react": "^2.0.17",
        "@mcansh/remix-raw-http": "^0.2.1",
        "@prisma/client": "^4.13.0",
        "@remix-run/node": "^1.16.0",
        "@remix-run/react": "^1.16.0",
        "@remix-run/serve": "^1.16.0",
        "@tailwindcss/forms": "^0.5.3",
        "@vercel/remix": "^1.16.0",
        "clsx": "^1.2.1",
        "html-escaper": "^3.0.3",
        "isbot": "latest",
        "react": "^18.2.0",
        "react-aria-components": "^1.0.0-alpha.3",
        "react-dom": "^18.2.0",
        "rehype-pretty-code": "^0.9.5",
        "rehype-slug": "^5.1.0",
        "remark-gfm": "^3.0.1",
        "shiki": "^0.14.2"
    },
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@remix-run/dev": "^1.16.0",
        "@remix-run/eslint-config": "^1.16.0",
        "@storybook/addon-actions": "^7.0.9",
        "@storybook/addon-essentials": "^7.0.9",
        "@storybook/addon-interactions": "^7.0.9",
        "@storybook/addon-links": "^7.0.9",
        "@storybook/core-server": "^7.0.9",
        "@storybook/react": "^7.0.9",
        "@storybook/react-vite": "^7.0.9",
        "@storybook/testing-library": "^0.1.0",
        "@testing-library/dom": "^8.19.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/html-escaper": "^3.0.0",
        "@types/postcss-import": "^14.0.0",
        "@vitejs/plugin-react": "^4.0.0",
        "autoprefixer": "^10.4.13",
        "babel-loader": "^8.3.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-storybook": "^0.6.12",
        "happy-dom": "^7.7.0",
        "husky": "^8.0.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.19",
        "postcss-import": "^15.0.0",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.8.0",
        "prettier-plugin-organize-imports": "^3.2.1",
        "prettier-plugin-prisma": "^4.13.0",
        "prettier-plugin-tailwindcss": "^0.2.0",
        "prisma": "^4.13.0",
        "storybook": "^7.0.9",
        "tailwindcss": "^3.2.4",
        "tsx": "^3.12.7",
        "typescript": "^4.7.4",
        "vite": "^4.3.0",
        "vite-tsconfig-paths": "^3.6.0",
        "vitest": "^0.25.3"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "singleQuote": false,
        "semi": false,
        "arrowParens": "avoid"
    }
}
