        "dev:remix": "remix dev",
        "build:unocss": "unocss app/**/*.tsx --out-file ./app/styles/uno.css",
        "build:remix": "remix build"
    },
    "resolutions": {
        "react": "npm:@preact/compat",
        "react-dom": "npm:@preact/compat"
    },
    "dependencies": {
        "@remix-run/node": "^1.7.2",
        "@remix-run/react": "^1.7.2",
        "@remix-run/serve": "^1.7.2",
        "esbuild": "^0.15.9",
        "preact": "^10.11.0",
        "preact-render-to-string": "^5.2.4",
        "react": "npm:@preact/compat",
        "react-dom": "npm:@preact/compat"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.7.2",
        "@remix-run/eslint-config": "^1.7.2",
        "eslint": "^8.23.1",
        "npm-run-all": "^4.1.5",
        "remix-flat-routes": "^0.4.8",
        "typescript": "^4.7.4",
        "unocss": "^0.46.5"
    },
    "engines": {
        "node": ">=14"
